   <style>
      .box_module{
           
           
           height: 40px;
           background-color: white;
      }
      .box_module h1{
         display: flex;
         justify-content: flex-start;
         position: relative;
         top: 10px;
         left: 5px;
      }
      .box_module h1 a{
         display: flex;
         justify-content: center;
         position: relative;
         top: 0px;
         left: 40px;
      }
      .box_module h2{
         display: flex;
         justify-content: flex-end;
        position: relative;
        right: 10px;
        top: -12px;
        font-size: 25px;
      }
      .logo .large-logo{
         width: 200px;
         height: 65px;
   
      }
      .logo .small-logo{
         
         width: 112px;
         height: 85px;
     }
      .logo img{
         width: 100%;
         height: 100%;
      }
      @media screen and (max-width: 1600px) {
         .logo .large-logo {
             display: block; /* Hide the large logo on small screens */
         }
         .logo .small-logo {
             display: none; /* Show the small logo on small screens */
         }
     }
     @media screen and (max-width: 600px) {
         .logo .large-logo {
             display: none; /* Hide the large logo on small screens */
         }
         .logo .small-logo {
             display: block; /* Show the small logo on small screens */
         }
     }
   </style>
</head>
